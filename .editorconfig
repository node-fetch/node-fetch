# editorconfig.org

root = true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
indent_style = tab

[*.md]
trim_trailing_whitespace = false

[*.yml]
indent_style = space

[package.json]
indent_style = space
indent_size = 2
insert_final_newline = false
